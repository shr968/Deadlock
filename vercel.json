{
    "version": 2,
    "builds": [
      {
        "src": "src/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "/public/index.html"
      },
      {
        "src": "/login",
        "dest": "/src/server.js"
      },
      {
        "src": "/signup",
        "dest": "/src/server.js"
      },
      {
        "src": "/thanks",
        "dest": "/src/server.js"
      },
      {
        "src": "/index",
        "dest": "/src/server.js"
      },
      {
        "src": "/form",
        "dest": "/src/server.js"
      },
      {
        "src": "/main",
        "dest": "/src/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/src/server.js"
      }
    ]
  }
  